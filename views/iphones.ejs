<script>
 
 function getitemss(a){
  const cartForm = document.getElementById(a);
  const formData = new FormData(cartForm);

  const cartItem = {};


  formData.forEach((value, key) => {
    console.log(key,value)
    cartItem[key] = value;
  });
  const selectedBrand = cartItem.phonebrand;
  const selectedModel = cartItem.phonemodel;

  // Find the corresponding price from phoneData based on selected brand and model
  const matchedItem = phoneData.find(item => item.brand === selectedBrand && item.model === selectedModel);

  // If a matching item is found, assign its price to cartItem
  if (matchedItem) {
    cartItem.price = matchedItem.price;
  } else {
    alert('Price not found for the selected brand and model.');
    return; // Exit if no matching price is found
  }
  cartItem.id = Date.now();

  let cartItems = localStorage.getItem('cartIT');
  if (!cartItems) {
    
    cartItems = [];
  } else {
   
    cartItems = JSON.parse(cartItems);
  }

  cartItems.push(cartItem);
  

 localStorage.setItem('cartIT', JSON.stringify(cartItems));
 console.log(cartItems)
 alert('Item added to cart:', cartItem);

  }

  const phoneData = JSON.parse('<%- JSON.stringify(ps) %>')

  
  const caseMaterialSelectors = document.querySelectorAll('.case_mat');
  const brandSelectors = document.querySelectorAll('.brandSelector');
  const modelSelectors = document.querySelectorAll('.modelSelector');
  
  for (let i = 0; i < brandSelectors.length; i++) {
      const caseMaterialSelector = caseMaterialSelectors[i];
      const brandSelector = brandSelectors[i];
      const modelSelector = modelSelectors[i];
  
      brandSelector.innerHTML = '<option value="">Select Brand</option>';
      const uniqueBrands = [...new Set(phoneData.map(item => item.brand))];
      uniqueBrands.forEach(brand => {
          const option = document.createElement('option');
          option.value = brand;
          option.text = brand;
          brandSelector.appendChild(option);
      });
  
      brandSelector.addEventListener('change', () => {
          const selectedBrand = brandSelector.value;
          const selectedCaseMaterial = caseMaterialSelector.value;
  
          if (selectedBrand) {
              const brandModels = phoneData.filter(item => item.brand === selectedBrand && item.case_material === selectedCaseMaterial).map(item => item.model);
  
              modelSelector.innerHTML = '<option value="">Select Model</option>';
              brandModels.forEach(model => {
                  const option = document.createElement('option');
                  option.value = model;
                  option.text = model;
                  modelSelector.appendChild(option);
              });
              
              modelSelector.disabled = false;
          } else {
              modelSelector.innerHTML = '<option value="">Select Model</option>';
              modelSelector.disabled = true;
          }
      });
  
      caseMaterialSelector.addEventListener('change', () => {
          const selectedBrand = brandSelector.value;
          const selectedCaseMaterial = caseMaterialSelector.value;
  
          if (selectedBrand) {
              const brandModels = phoneData.filter(item => item.brand === selectedBrand && item.case_material === selectedCaseMaterial).map(item => item.model);
  
              modelSelector.innerHTML = '<option value="">Select Model</option>';
              brandModels.forEach(model => {
                  const option = document.createElement('option');
                  option.value = model;
                  option.text = model;
                  modelSelector.appendChild(option);
              });
              modelSelector.disabled = false;
          } else {
              modelSelector.innerHTML = '<option value="">Select Model</option>';
              modelSelector.disabled = true;
          }
      });
  }
  </script>
</script>
